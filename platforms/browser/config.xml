<?xml version='1.0' encoding='utf-8'?>
<widget id="us.uni.project" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>UTBM INFO</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="GOOGLE_MAPS_ANDROID_API_KEY" value="AIzaSyAH6eEhX9nywS8gKW3ATY7Cyg-Gqj9-ufY" />
    <preference name="GOOGLE_MAPS_IOS_API_KEY" value="(api key)" />
    <preference name="phonegap-version" value="cli-8.1.1" />
    <preference name="SplashScreen" value="www/img/splash/AndroidSplash.png" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="ShowSplashScreenSpinner" value="false" />
</widget>
